<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#D5A6BD"
    tools:context=".EditProfileActivity">

    <ImageView
        android:id="@+id/imageView11"
        android:layout_width="80dp"
        android:layout_height="89dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_alignParentBottom="true"
        android:layout_marginStart="16dp"
        android:layout_marginTop="40dp"
        android:src="@drawable/logo"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/edit_email"
        android:layout_width="209dp"
        android:layout_height="40dp"
        android:background="#8AF1ECEC"
        android:ems="10"
        android:inputType="textPersonName"
        android:minHeight="40dp"
        android:text="Correo"
        android:textAlignment="center"
        android:textColor="#FF803e5b"
        android:textColorHint="#FF803e5b"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.476"
        tools:ignore="MissingConstraints,TouchTargetSizeCheck" />

    <EditText
        android:id="@+id/edit_name"
        android:layout_width="209dp"
        android:layout_height="40dp"
        android:background="#8AF1ECEC"
        android:backgroundTint="@null"
        android:ems="10"
        android:inputType="textPersonName"
        android:minHeight="40dp"
        android:text="Nombre"
        android:textAlignment="center"
        android:textColor="#FF803e5b"
        android:textColorHint="#FF803e5b"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.293"
        tools:ignore="MissingConstraints,TouchTargetSizeCheck" />

    <EditText
        android:id="@+id/edit_address"
        android:layout_width="209dp"
        android:layout_height="40dp"
        android:background="#8AF1ECEC"
        android:ems="10"
        android:inputType="textPersonName"
        android:minHeight="40dp"
        android:text="Direccion"
        android:textAlignment="center"
        android:textColor="#FF803e5b"
        android:textColorHint="#FF803e5b"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.385"
        tools:ignore="MissingConstraints,TouchTargetSizeCheck" />

    <EditText
        android:id="@+id/edit_password"
        android:layout_width="209dp"
        android:layout_height="40dp"
        android:background="#8AF1ECEC"
        android:ems="10"
        android:inputType="textPersonName"
        android:minHeight="40dp"
        android:text="ContraseÃ±a"
        android:textAlignment="center"
        android:textColor="#FF803e5b"
        android:textColorHint="#FF803e5b"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.658"
        tools:ignore="MissingConstraints,TouchTargetSizeCheck" />

    <EditText
        android:id="@+id/edit_phone"
        android:layout_width="209dp"
        android:layout_height="40dp"
        android:background="#8AF1ECEC"
        android:ems="10"
        android:inputType="textPersonName"
        android:minHeight="40dp"
        android:text="Telefono"
        android:textAlignment="center"
        android:textColor="#FF803e5b"
        android:textColorHint="#FF803e5b"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.495"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.566"
        tools:ignore="MissingConstraints,TouchTargetSizeCheck" />

    <Button
        android:id="@+id/btnsaveeditprofile"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Actualizar"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.487"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.751"
        tools:ignore="MissingConstraints" />

    <ImageView
        android:id="@+id/imageView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/fotop"
        app:layout_constraintBottom_toTopOf="@+id/edit_name"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.978" />

    <Button
        android:id="@+id/cambiar_foto"
        android:layout_width="84dp"
        android:layout_height="31dp"
        android:text="Cambiar"
        android:textSize="10sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.56"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.226"
        tools:ignore="MissingConstraints,TouchTargetSizeCheck" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/linearLayout"
        android:layout_width="414dp"
        android:layout_height="57dp"
        android:layout_marginBottom="-10dp"
        android:background="#C3F6F3F6"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.666"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.982">

        <ImageButton
            android:id="@+id/btncasa"
            android:layout_width="25dp"
            android:layout_height="25dp"
            android:layout_marginStart="58dp"
            android:background="#C3F6F3F6"
            android:backgroundTint="#C3F6F3F6"
            android:baselineAligned="false"
            android:foregroundGravity="center"
            android:src="@drawable/home2"
            android:textAlignment="viewEnd"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:tint="#FF803e5b"
            tools:ignore="MissingConstraints,SpeakableTextPresentCheck,TouchTargetSizeCheck" />

        <ImageButton
            android:id="@+id/imageButton16"
            android:layout_width="25dp"
            android:layout_height="25dp"
            android:layout_marginStart="65dp"
            android:layout_marginBottom="16dp"
            android:background="#C3F6F3F6"
            android:backgroundTint="#C3F6F3F6"
            android:src="@drawable/instagram__1_"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toEndOf="@+id/btncasa"
            app:tint="#FF803e5b"
            tools:ignore="MissingConstraints,SpeakableTextPresentCheck,TouchTargetSizeCheck" />

        <ImageButton
            android:id="@+id/btncarrito"
            android:layout_width="25dp"
            android:layout_height="25dp"
            android:layout_marginStart="65dp"
            android:layout_marginBottom="16dp"
            android:background="#C3F6F3F6"
            android:backgroundTint="#C3F6F3F6"
            android:src="@drawable/shopping_cart"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toEndOf="@+id/imageButton16"
            app:tint="#FF803e5b"
            tools:ignore="MissingConstraints,SpeakableTextPresentCheck,TouchTargetSizeCheck" />

        <ImageButton
            android:id="@+id/btnprofile"
            android:layout_width="25dp"
            android:layout_height="25dp"
            android:layout_marginStart="65dp"
            android:layout_marginEnd="70dp"
            android:layout_marginBottom="16dp"
            android:background="#C3F6F3F6"
            android:backgroundTint="#C3F6F3F6"
            android:src="@drawable/user__1_"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toEndOf="@+id/btncarrito"
            app:tint="#FF803e5b"
            tools:ignore="MissingConstraints,SpeakableTextPresentCheck,TouchTargetSizeCheck" />
    </androidx.constraintlayout.widget.ConstraintLayout>

</androidx.constraintlayout.widget.ConstraintLayout>
