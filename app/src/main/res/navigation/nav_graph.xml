<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/homeFragment">

    <fragment
        android:id="@+id/homeFragment"
        android:name="com.example.tiendafreshstart.HomeFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_home" >
        <action
            android:id="@+id/action_homeFragment_to_formsFragment"
            app:destination="@id/formsFragment" />
        <action
            android:id="@+id/action_homeFragment_to_editProfileFragment"
            app:destination="@id/editProfileFragment" />
        <action
            android:id="@+id/action_homeFragment_to_mapsActivity"
            app:destination="@id/mapsActivity" />
        <action
            android:id="@+id/action_homeFragment_to_carritoFragment2"
            app:destination="@id/carritoFragment2" />
    </fragment>
    <fragment
        android:id="@+id/formsFragment"
        android:name="com.example.tiendafreshstart.FormsFragment"
        android:label="fragment_forms"
        tools:layout="@layout/fragment_forms" >
        <action
            android:id="@+id/action_formsFragment_to_registerFragment"
            app:destination="@id/registerFragment" />
        <action
            android:id="@+id/action_formsFragment_to_loginFragment"
            app:destination="@id/loginFragment" />
        <action
            android:id="@+id/action_formsFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
    </fragment>
    <fragment
        android:id="@+id/registerFragment"
        android:name="com.example.tiendafreshstart.RegisterFragment"
        android:label="fragment_register"
        tools:layout="@layout/fragment_register" >
        <action
            android:id="@+id/action_registerFragment_to_loginFragment"
            app:destination="@id/loginFragment" />
        <action
            android:id="@+id/action_registerFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
    </fragment>
    <fragment
        android:id="@+id/loginFragment"
        android:name="com.example.tiendafreshstart.LoginFragment"
        android:label="fragment_login"
        tools:layout="@layout/fragment_login" >
        <action
            android:id="@+id/action_loginFragment_to_productsFragment"
            app:destination="@id/productsFragment" />
        <action
            android:id="@+id/action_loginFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
        <action
            android:id="@+id/action_loginFragment_to_registerFragment"
            app:destination="@id/registerFragment" />
    </fragment>
    <fragment
        android:id="@+id/productsFragment"
        android:name="com.example.tiendafreshstart.ProductsFragment"
        android:label="fragment_products"
        tools:layout="@layout/fragment_products" >
        <action
            android:id="@+id/action_productsFragment_to_product_detailsFragment"
            app:destination="@id/product_detailsFragment" />
        <action
            android:id="@+id/action_productsFragment_to_editProfileFragment"
            app:destination="@id/editProfileFragment" />
        <action
            android:id="@+id/action_productsFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
        <action
            android:id="@+id/action_productsFragment_to_carritoFragment2"
            app:destination="@id/carritoFragment2" />
    </fragment>
    <fragment
        android:id="@+id/product_detailsFragment"
        android:name="com.example.tiendafreshstart.product_detailsFragment"
        android:label="fragment_product_details"
        tools:layout="@layout/fragment_product_details" >
        <action
            android:id="@+id/action_product_detailsFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
        <action
            android:id="@+id/action_product_detailsFragment_to_editProfileFragment"
            app:destination="@id/editProfileFragment" />
        <action
            android:id="@+id/action_product_detailsFragment_to_comentariosFragment"
            app:destination="@id/comentariosFragment" />
        <action
            android:id="@+id/action_product_detailsFragment_to_carritoFragment23"
            app:destination="@id/carritoFragment2" />
    </fragment>
    <fragment
        android:id="@+id/editProfileFragment"
        android:name="com.example.tiendafreshstart.EditProfileFragment"
        android:label="fragment_edit_profile"
        tools:layout="@layout/fragment_edit_profile" >
        <action
            android:id="@+id/action_editProfileFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
        <action
            android:id="@+id/action_editProfileFragment_to_carritoFragment2"
            app:destination="@id/carritoFragment2" />
    </fragment>
    <activity
        android:id="@+id/mapsActivity"
        android:name="com.example.tiendafreshstart.MapsActivity"
        android:label="activity_maps"
        tools:layout="@layout/activity_maps" />
    <fragment
        android:id="@+id/comentariosFragment"
        android:name="com.example.tiendafreshstart.ComentariosFragment"
        android:label="fragment_comentarios"
        tools:layout="@layout/fragment_comentarios" >
        <action
            android:id="@+id/action_comentariosFragment_to_editProfileFragment"
            app:destination="@id/editProfileFragment" />
        <action
            android:id="@+id/action_comentariosFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
        <action
            android:id="@+id/action_comentariosFragment_to_carritoFragment2"
            app:destination="@id/carritoFragment2" />
        <action
            android:id="@+id/action_comentariosFragment_to_product_detailsFragment"
            app:destination="@id/product_detailsFragment" />
    </fragment>
    <fragment
        android:id="@+id/carritoFragment2"
        android:name="com.example.tiendafreshstart.CarritoFragment"
        android:label="CarritoFragment" >
        <action
            android:id="@+id/action_carritoFragment2_to_homeFragment"
            app:destination="@id/homeFragment" />
        <action
            android:id="@+id/action_carritoFragment2_to_editProfileFragment"
            app:destination="@id/editProfileFragment" />
    </fragment>
</navigation>